{
  "name": "ajinware-backend",
  "version": "1.2.0",
  "description": "API design for Mountain Rail System",
  "main": "index.js",
  "engine": {
    "node": "v20.17.0",
    "npm": "*",
    "yarn": "please-use-npm",
    "pnpm": "please-use-npm"
  },
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "format:check": "prettier --check src",
    "format": "prettier --write src --cache",
    "lint:check": "eslint src/**/*.ts --fix --quiet",
    "lint": "eslint --fix src/**/*.ts",
    "code:check": "npm run format:check && npm run lint:check",
    "code:fix": "npm run format && npm run lint",
    "test": "jest"
  },
  "license": "MIT",
  "dependencies": {
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.21.0",
    "helmet": "7.1.0",
    "morgan": "1.10.0"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.13",
    "@types/morgan": "1.9.9",
    "@types/node": "22.5.5",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "8.6.0",
    "@typescript-eslint/parser": "8.6.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.30.0",
    "jest": "29.7.0",
    "nodemon": "3.1.5",
    "prettier": "3.3.3",
    "prettier-plugin-organize-imports": "4.1.0",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.6.2"
  }
}
